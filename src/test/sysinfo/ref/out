GetBasename("")=.
GetBasename("./")=.
GetBasename("./.")=.
GetBasename("a/b")=b
GetBasename("a")=a
GetDirname("")=.
GetDirname("./")=.
GetDirname("./.")=.
GetDirname("a/b")=a
GetDirname("a")=.
IsFile("test")=1
IsFile("main2.cpp")=0
GetRealpath("main2.cpp")=
Join("a", "")=a
Join("", "b")=b
Join("a", "b")=a/b
Join("a/", "b")=a/b
Join("a", "/b")=/b
IsDir(".")=1
SplitExt(".a")=(.a, )
SplitExt("a.b")=(a, .b)
SplitExt("a/.b")=(a/.b, )
SplitExt("a/b.c")=(a/b, .c)
SplitExt("a/b.c.d")=(a/b.c, .d)
SplitExt("a")=(a, )
